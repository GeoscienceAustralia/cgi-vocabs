<http://resource.geosciml.org/classifier/cgi/exploration-result>
    a skos:Collection ;
    schema:citation "The original vocabulary - all its concepts"@en ;
    skos:changeNote
        "2021-12-17 R.Heaven BGS Added alignments to INSPIRE registry codelists using new alignments created automatically using Levenshtein similarity threshold of 0.75."@en ;
    skos:definition "All the Concepts in this vocabulary"@en ;
    skos:member
        :favorable-geological-environment,
        :geochemical-anomaly,
        :geophysical-anomaly,
        :identification-anomalous-area,
        :key-geological-features,
        :mineral-occurrences,
        :mineral-reserve-defined,
        :mineral-resource-defined,
        :mineral-resource-indicated,
        :mineralized-zone-identified,
        :negative-feasibility-study,
        :positive-feasibility-study ;
    skos:prefLabel "Exploration Result - All Concepts"@en
