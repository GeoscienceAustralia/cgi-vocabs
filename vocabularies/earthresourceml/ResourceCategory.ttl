    skos:definition "Poorly estimated mineral resource, poorly documented"@en ;
    skos:inScheme cs: ;
    skos:notation "07"^^dt:LocalHierarchyKey ;
    skos:prefLabel "poorly estimated mineral resource, poorly documented"@en ;
    skos:topConceptOf cs:
