    skos:inScheme cs: ;
    skos:notation "02.."^^dt:LocalHierarchyKey ;
    skos:prefLabel "Schicht"@de,
        "tabular"@en,
        "estrato"@es,
        "kerrokset"@fi,
        "strato"@it,
