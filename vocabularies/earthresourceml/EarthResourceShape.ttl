:cylindrical a skos:Concept ;
    schema:citation "BGS Mineral Occurrence Database"@en ;
    rdfs:isDefinedBy cs: ;
    skos:altLabel "chimney"@en,
        "funnel"@en,
        "pipe"@en,
        "pipe-like"@en,
        "shoot"@en,
        "tube"@en,
        "tube-like"@en,
        "tubular"@en ;
    skos:definition "A roughly cylindrical body, which may reduce in diameter at one end like a funnel"@en ;
    skos:inScheme cs: ;
    skos:notation "05.."^^dt:LocalHierarchyKey ;
    skos:prefLabel "Pipe"@de,
        "cylindrical"@en,
        "pipa"@es,
        "piiput"@fi,
        "tubo"@it,
        "trubka"@ru,
        "pipe"@sv ;
    skos:topConceptOf cs: .


:irregular a skos:Concept ;
    schema:citation "From CGI GeologicUnitMorphology vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "Body geometry is irregular and can not be characterized using terminology."@en ;
    skos:inScheme cs: ;
    skos:notation "07.."^^dt:LocalHierarchyKey ;
    skos:prefLabel "irregular"@en ;
    skos:topConceptOf cs: .


:lensoidal a skos:Concept ;
