        :auger-drilling,
        :core-drilling,
        :excavation,
        :percussion-drilling ;
    skos:notation "03"^^dt:LocalHierarchyKey ;
    skos:prefLabel "subsurface exploration"@en ;
    skos:topConceptOf cs:
.


