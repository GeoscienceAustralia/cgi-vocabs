:arch_morphology a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "A raised elongate body, morphologically equivalent to upside down channel"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "arch morphology"@en ;
    skos:related :longitudinal_section_of_cylinder .


:arcuate_sheet a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :sheet ;
    skos:definition "Sheet in which one of L or W is a curved arc"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "arcuate sheet"@en .


:basin_shape a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "Geometric analog is convex down discoid, term generally connotes a kilometer-scale feature. Map view may be circular, elliptical, or irregular, characteristic feature is thickening from the edges towards the center. Typically used to describe a body of sedimentary rock deposited in a low area in the Earth's crust, in which case there is an implication that the top is (or was) a surface of deposition."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "basin shape"@en,
        "buffertzon"@sv ;
    skos:related :discus,
        :hemispheroid .


:blanket_shape a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "A thin, widespread body with width to thickness ratio greater than 1000 to 1 that covers an underlying substrate. A layer of known great lateral extent. Morphology equivalent to bed or layer, but implies that body forms the top of some unit, instead of occurring within a body."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "blanket shape"@en ;
    skos:related :sheet .


:blob a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :three_dimensional_body ;
    skos:definition "Body with smooth surface, broadly equant shape, irregular protuberances, no symmetry axes, and a boundary that can not be separated into sides."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "blob"@en .


:block a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:altLabel "bangun bongkah"@id,
        "지괴구조"@ko ;
    skos:broader :polygonal_cylinder ;
    skos:definition "Equant polygonal cylinder, L=W=H, definable edges separate bounding surfaces. Grades to tabular prism as one horizontal dimension becomes distinctly longer than the other, and tablet or polygonal cylinder as one vertical axis becomes distinctly shorter or longer than the horizontal axes."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Blockstruktur"@de,
        "block"@en,
        "estructura bloques"@es,
        "lohkorakenteet"@fi,
        "structure bloc"@fr,
        "struktur bongkah"@id,
        "struttura a blocchi"@it,
        "地塊構造"@ja,
        "rcnasm<½nCabøúk"@km,
        "암괴구조"@ko,
        "Â£¤¦É¾¤»ø®®ìñº¡"@lo,
        "struktur bongkah"@ms,
        "struktura blokovaya"@ru,
