    skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/planar_profile> ;
    skos:definition "Profile for which the ratio of length of longest chord to shortest chord is less than 1.4."@en ;
    skos:historyNote "This vocabulary"@en ;
    skos:inScheme cs: ;
    skos:narrower
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/circular_profile> ,
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/equant_polygonal_profile> ;
    skos:prefLabel "equant profile"@en ;
.

<http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/sill_form>
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/geologic_body> ;
    skos:definition "A tabular intrusive igneous body with long dimensions parallel to bedding or foliation of sedimentary or metamorphic host rock."@en ;
    skos:historyNote "This vocabulary"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "sill form"@en ;
    skos:related
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/layer_shape> ,
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/sheet> ,
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/sheet_ellipsoid> ;
.

<http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/wedge>
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/three_dimensional_body> ;
    skos:definition "A wedge is a polyhedron defined by two triangles and three trapezoid faces. A wedge has five faces, nine edges, and six vertices. A wedge is a subclass of the prismatoids with the base and opposite ridge in two parallel planes."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "wedge"@en ;
    skos:related <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/prism> ;
    schema:citation "https://en.wikipedia.org/wiki/Wedge_(geometry)"^^xsd:anyURI ;
.

<http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/hemispheroid>
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/three_dimensional_body> ;
    skos:definition "Section of a spheroid formed by removing everything on the smaller volume side of a surface that intersects the spheroid. Grades to cone as suface generator (line rotated about axis to generate volume above lower planar boundary) becomes straight."@en ;
    skos:historyNote "This vocabulary"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "hemispheroid"@en ;
.

<http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/layer_shape>
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:altLabel
        "Schicht"@de ,
        "estrato"@es ,
        "kerrokset"@fi ,
        "strato"@it ,
        "sloj"@ru ,
        "lager"@sv ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/geologic_body> ;
    skos:definition "Body geometry characterized by generally thin character relative to the lateral extent of the body (on order of 1:100). Thin bodies of lesser extent are lenses. Connotes a non-intrusive body, equivalent intrusive body would be dike (or sill). Denotes that body is in a sequence of similar sheet-like bodies between other rock bodies"@en ;
    skos:historyNote "This vocabulary"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "layer shape"@en ;
    skos:related
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/blanket_shape> ,
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/sill_form> ;
.

<http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/polygonal_cylinder>
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/cylinder> ;
    skos:definition "Cylinder in which the edges bounding side surfaces are parallel or subparallel to cylinder axis. Cross section is polygon. Length of axis may be called height or thickness, and is typically in the vertical direction (distinguishing polygonal cylinder from tabular prism and wedge)."@en ;
    skos:historyNote "This vocabulary"@en ;
    skos:inScheme cs: ;
    skos:narrower
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/block> ,
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/square_profile> ,
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/tablet> ;
    skos:prefLabel "polygonal cylinder"@en ;
.

<http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/prism>
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/three_dimensional_body> ;
    skos:definition "A polyhedron with an n-sided polygonal base, another congruent parallel base (with the same rotational orientation), and n other faces (necessarily all parallelograms) joining corresponding sides of the two bases. All cross-sections parallel to the base faces are congruent to the bases. Prisms are named for their base, so a prism with a pentagonal base is called a pentagonal prism."@en ;
    skos:inScheme cs: ;
    skos:narrower <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/tabular_prism> ;
    skos:prefLabel "prism"@en ;
    skos:related <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/wedge> ;
    schema:citation "https://en.wikipedia.org/wiki/Prism_(geometry)"^^xsd:anyURI ;
.

<http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/sheet_ellipsoid>
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/ellipsoid> ;
    skos:definition "Ellipsoid with one axis much smaller than others H on order of .001 of L or W"@en ;
    skos:historyNote "This vocabulary"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "sheet ellipsoid"@en ;
.

<http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/vein>
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/geologic_body> ;
    skos:definition "A tabular or sheetlike epigenetic mineral filling of a fault or fracture"@en ;
    skos:historyNote "This vocabulary"@en ;
    skos:inScheme cs: ;
    skos:narrower
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/planar_vein> ,
