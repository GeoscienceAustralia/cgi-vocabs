        "โครงสร้างบล็อก"@th,
        "cấu trúc khối"@vi,
        "块状构造"@zh .


:boudin_shape a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "Layer or bed of rock deformed into sausage-shaped segments (boudins), either separated or joined by pinched connections. Morphology generally defined in a profile view, third dimension is not denoted, use of term also implies that the body is not isolated, but is related to an original body elongate on some dimension that has been stretched and separated into separate bodies"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "boudin shape"@en ;
    skos:related :bread_loaf_ellipsoid .


:bread_loaf_ellipsoid a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :ellipsoid ;
    skos:definition "Ellipsoid with L 10 times W or H"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "bread loaf ellipsoid"@en .


:channel_shape a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:altLabel "gully"@sv ;
    skos:broader :geologic_body ;
    skos:definition "Strongly elongate lens or ribbon with a convex downward lower surface, near planar upper surface. Morphology is equivalent to upside down arch."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Rinne"@de,
        "channel shape"@en,
        "canal natural"@es,
        "uomat"@fi,
        "canale"@it,
        "ruslo"@ru,
        "kanal"@sv ;
    skos:related :longitudinal_section_of_cylinder,
        :ribbon .


:circular_cylinder a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :elliptical_cylinder ;
    skos:definition "Elliptical cylinder that has circular symmetry about long axis, distinct side surfaces are not defined. Cross section is circular"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "circular cylinder"@en .


:circular_profile a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :equant_profile ;
    skos:definition "equant profile with arcuate boundary"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "circular profile"@en .


:column_shape a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "Cylindrical body, equant cross section, long in direction normal to equant cross section. Morphology equivalent to pipe, without denotation of transport function."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "column shape"@en ;
    skos:related :circular_cylinder,
        :polygonal_cylinder .


:cone a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :discus ;
    skos:definition "Discus with one planar side, and circular symmetry on axis, geologic cones typically have height <(<) diameter"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "cone shape"@en .


:cone_segment a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :three_dimensional_body ;
    skos:definition "Body that is section of a cone formed by splitting the cone along a surface parallel to the cone axis, that interesect approximately along the axis."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "cone segment"@en .


:conical_sheet a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :sheet ;
    skos:definition "Sheet that conforms to surface of a cone."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "conical sheet"@en .


:cupola a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "Large upward projection of the roof of an igneous intrusion into the country rock above. A possibly irregular dome like or columnar body rooted in a larger body."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "cupola"@en ;
    skos:related :hemispheroid .


:cylinder a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :three_dimensional_body ;
    skos:definition "Body defined by translation of a generating plane figure with approximately equant cross section (circle, ellipsoid, polygon) along a straight axial line. H>=W=L. Axis is direction normal to generating plane figure, which forms parallel top and bottom bounding surface."@en ;
    skos:inScheme cs: ;
    skos:narrower :disc,
        :elliptical_cylinder,
        :polygonal_cylinder,
        :rod ;
    skos:prefLabel "cylinder"@en .


:dart_ellipsoid a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :ellipsoid ;
    skos:definition "Ellipsoid with L more than 50 times W or H"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "dart ellipsoid"@en .


:diapir a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "Broadly inverted tear drop morphology. Body of magmatic or mobile sedimentary material intruded into overlying rocks."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "diapir"@en .


:dike a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:altLabel "dyke"@en ;
    skos:broader :geologic_body ;
    skos:definition "A generally sheet-like intrusive rock body that cuts across bedding or foliation in the host rock, or intrudes massive host rock."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Dyke"@de,
        "dike"@en,
        "dique"@es,
        "juonet"@fi,
        "dicco"@it,
        "dajka"@ru,
        "gång"@sv ;
    skos:related :sheet .


:disc a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :cylinder ;
    skos:definition "Cylinder with height less than diameter. L=W>H, edges separates side from top and bottom. Grades to oblate ellipsoid as edges lose definition."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "disc"@en .


:discoid_anulus a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :three_dimensional_body ;
    skos:definition "Cone with central part removed along cylindrical volume approximately along the cone axis"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "discoid anulus"@en .


:discus a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :three_dimensional_body ;
    skos:definition "Body with two sides that converge to define an edge that is circular in profile, edge separates definable top and bottom. L=W>H. Grades to oblate ellipsoid as edge separating sides loses definition, cone as one side becomes distinctly planar."@en ;
    skos:inScheme cs: ;
    skos:narrower :cone,
        :elongate_discus ;
    skos:prefLabel "discus"@en .


:dome a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "Steep-sided disk-like body, for example and accumulation of high viscosity lava above and around a volcanic vent. A structure in which the top of the body dips gently away in all directions e.g. salt dome, extrusion dome"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Dom"@de,
        "dome"@en,
        "domo"@es,
        "doomit"@fi,
        "duomo"@it,
        "kupol"@ru,
        "dom"@sv ;
    skos:related :hemispheroid .


:dome_sheet a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :sheet ;
    skos:definition "Sheet in which L and W are curved arcs."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "dome sheet"@en .


:ductolith a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "Concordant intrusion, teardrop-shaped in cross section."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "ductolith"@en ;
    skos:related :diapir,
        :tear_drop .


:ellipsoid a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :three_dimensional_body ;
    skos:definition "Body that contains three orthogonal axes defining three surfaces of reflection symmetry, profiles along the surfaces are elliptical. Boundary can not be separated into sides (no edges). Becomes cylinder or disc when bounding surface can be considered parallel to one of the axis. L>=W>=H"@en ;
    skos:inScheme cs: ;
    skos:narrower :bread_loaf_ellipsoid,
        :dart_ellipsoid,
        :football_ellipsoid,
        :pancake_ellipsoid,
        :pillow_ellipsoid,
        :sheet_ellipsoid,
        :spheroid ;
    skos:prefLabel "ellipsoid"@en .


:elliptical_cylinder a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :cylinder ;
    skos:definition "Cylinder that has elliptical cross section, distinct side surfaces are not defined."@en ;
    skos:inScheme cs: ;
    skos:narrower :circular_cylinder ;
    skos:prefLabel "elliptical cylinder"@en .


:elliptical_profile a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :planar_profile ;
    skos:definition "Profile approximates an ellipse, ratio of long axis to short axis is greater than 1.4"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "elliptical profile"@en .


:elongate_discus a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :discus ;
    skos:definition "Discus for which the profile defined by the bounding edge is longer in one direction than other. L>W>>H"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "elongate discus"@en .


:equant_polygonal_profile a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :equant_profile ;
    skos:definition "Aquant profile with boundary that can be divided into distinct straight sides separated by higher curvature corners."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "equant polygonal profile"@en .


:equant_profile a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :planar_profile ;
    skos:definition "Profile for which the ratio of length of longest chord to shortest chord is less than 1.4."@en ;
    skos:inScheme cs: ;
    skos:narrower :circular_profile,
        :equant_polygonal_profile ;
    skos:prefLabel "equant profile"@en .


:ethmolith a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "Discordant, funnel-shaped intrusion, tapers downward"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "ethmolith"@en ;
    skos:related :cone .


:fan a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "A cone segment, generally applied to surface morphology of sedimentary deposit with a gentle surface slope, radiating from a localized sediment source."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Faecher"@de,
        "fan"@en,
        "cono"@es,
        "keilat"@fi,
        "conoide"@it,
        "vynos veeroobraznyj"@ru,
        "kon"@sv ;
    skos:related :cone_segment .


:football_ellipsoid a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :ellipsoid ;
    skos:definition "Ellipsoid with L 2 to 5 times W or H"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "football ellipsoid"@en .


