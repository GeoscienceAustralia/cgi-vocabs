:graben_shape a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :geologic_body ;
    skos:definition "an elongate body bounded on both sides by normal faults that dip toward each other"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Graben"@de,
        "graben shape"@en,
        "graben"@es,
        "hautavajoamat"@fi,
        "graben"@it,
        "graben"@ru,
        "gravsänka"@sv ;
    skos:related :tabular_prism .


:hemispheroid a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :three_dimensional_body ;
