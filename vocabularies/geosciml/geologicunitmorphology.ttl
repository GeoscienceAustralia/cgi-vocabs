:planar_profile a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "Shape is defined by intersection of body with some surface, generally either a cross section or map view"@en ;
    skos:inScheme cs: ;
    skos:narrower :elliptical_profile,
        :equant_profile,
        :irregular_profile,
        :lenticular_profile,
        :polygonal_profile ;
    skos:prefLabel "planar profile"@en ;
    skos:topConceptOf cs: .


