    skos:prefLabel "vein-shape"@en ;
    skos:related <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/sheet> ;
.

<http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/cylinder>
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/three_dimensional_body> ;
    skos:definition "Body defined by translation of a generating plane figure with approximately equant cross section (circle, ellipsoid, polygon) along a straight axial line. H>=W=L. Axis is direction normal to generating plane figure, which forms parallel top and bottom bounding surface."@en ;
    skos:historyNote "This vocabulary"@en ;
    skos:inScheme cs: ;
    skos:narrower
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/disc> ,
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/elliptical_cylinder> ,
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/polygonal_cylinder> ,
        <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/rod> ;
    skos:prefLabel "cylinder"@en ;
.

<http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/longitudinal_section_of_cylinder>
    a skos:Concept ;
    rdfs:isDefinedBy cs: ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geologicunitmorphology/three_dimensional_body> ;
    skos:definition "Body that is section of a circular or elliptical cylinder formed by a splitting the cylinder along a surface approximately parallel to its long axis. L>>W>H."@en ;
    skos:historyNote "This vocabulary"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "longitudinal section of cylinder"@en ;
