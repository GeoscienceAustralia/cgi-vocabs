:compound_material a skos:Concept ;
    schema:citation "NADM C1 2004"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "An Earth Material composed of an aggregation of particles of Earth Material, possibly including other compound Materials. This is 'top' of lithology category hierarchy, and should be used to indicate 'any rock or unconsolidated material'."@en ;
    skos:exactMatch <http://inspire.ec.europa.eu/codelist/LithologyValue/compoundMaterial> ;
    skos:inScheme cs: ;
    skos:narrower :anthropogenic_material,
        :breccia,
        :composite_genesis_material,
        :igneous_material,
        :rock,
        :sedimentary_material,
        :unconsolidated_material ;
    skos:prefLabel "compound material"@en ;
    skos:topConceptOf cs: .


