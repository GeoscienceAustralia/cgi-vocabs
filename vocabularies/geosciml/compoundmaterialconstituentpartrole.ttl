    skos:broader <http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/particulate_constituent> ;
    skos:definition "A crystal of an earlier rock that is prevented from further reaction in a later rock by a rim of reaction products"@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:prefLabel "armoured relict crystal"@en .


