:constituent_role_not_specified a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "For descriptions in which role of constituent in compound material is not specified."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "constituent role not specified"@en ;
    skos:topConceptOf cs: .


:interstitial_constituent a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "Constituent is distributed through the material between other constituent particles"@en ;
    skos:inScheme cs: ;
    skos:narrower :groundmass_constituent,
        :matrix,
        :sedimentary_rock_cement ;
    skos:prefLabel "interstitial constituent"@en ;
    skos:topConceptOf cs: .


:particulate_constituent a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "Constituent occurs as a collection of particles that are characterized by average properties of the individual particles"@en ;
    skos:inScheme cs: ;
    skos:narrower :armoured_relict_crystal,
        :chadacryst,
        :crystalline_framework_constituent,
        :granular_framework_constituent,
        :megacryst,
        :oikocryst,
        :phenoclast ;
    skos:prefLabel "particulate constituent"@en ;
    skos:topConceptOf cs: .


:structural_part a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "Constituent occurs in a structural configuration integral to the rock,such as layering,veinlets,overgrowths. The 'material' composition of these parts will often be other rock materials,not minerals,and 'ParticleGeometryDescription' associated with these describes the geometry of the constituent,not the particles the it is made of. These roles are mostly useful for RockMaterial descriptions that apply to individual samples,because their distribution is unlikely to be pervasive enought to be considered characteristic of a large mass of material."@en ;
    skos:inScheme cs: ;
    skos:narrower :clot,
        :corona,
        :dropstone,
        :floating_clast,
        :inclusion,
        :irregular_part,
        :layer,
        :overgrowth,
        :small_concretion,
        :vein ;
    skos:prefLabel "structural part"@en ;
    skos:topConceptOf cs: .


:armoured_relict_crystal a skos:Concept ;
    schema:citation "base on Neuendorf et al 2005"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :particulate_constituent ;
    skos:definition "A crystal of an earlier rock that is prevented from further reaction in a later rock by a rim of reaction products"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "armoured relict crystal"@en .


:armoured_relict_inclusion a skos:Concept ;
    schema:citation "base on Neuendorf et al 2005"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :inclusion ;
    skos:definition "an inclusion of an earlier rock that is prevented from further reaction in a later rock by a rim of reaction products"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "armoured relict inclusion"@en .


:chadacryst a skos:Concept ;
    schema:citation "Neuendorf et al.,2005"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :particulate_constituent ;
    skos:definition "The enclosed crystal in a poikolitic texture"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "chadacryst"@en .


:clast_supporting_orthomatrix a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :orthomatrix ;
    skos:definition "Orthomatrix in matrix supported sedimentary rock."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "clast supporting orthomatrix"@en .


:clot a skos:Concept ;
    schema:citation "Neuendorf et al,2005"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :structural_part ;
    skos:definition "A cluster of ferromagnesian minerals in an igneous rock,from several centimeters to decimeters in diameter,that may be a segregation or an altered xenolith."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "clot"@en .


:corona a skos:Concept ;
    schema:citation "Williams,Turner and Gilbert,1954, Spry,1969, Fettes and Desmons,2007"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :structural_part ;
    skos:definition "Constituent occurs as a concentric envelope enclosing another constituent. Corona is a non-genetic term."@en ;
    skos:inScheme cs: ;
    skos:narrower :corrosion_rim,
        :kelyphitic_rim ;
    skos:prefLabel "corona"@en .


:corrosion_rim a skos:Concept ;
    schema:citation "Neuendorf et al.,2005"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :corona ;
    skos:definition "A corona formed by a secondary mineral around an orginal igneous crystal,formed by modification of the crystal by the corrosive action of its parent magma"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "corrosion rim"@en .


:crystalline_framework_constituent a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :particulate_constituent ;
    skos:definition "Constituent forms an interconnected network of discernible crystals."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "crystalline framework constituent"@en .


:dropstone a skos:Concept ;
    schema:citation "NADM SLTTs 2004, Jackson,1997,p 193"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :structural_part ;
    skos:definition "An oversized stone in laminated sediment that depresses the underlying laminae and may be covered by -draped laminae. Most dropstones originate through ice-rafting, other sources are floating tree roots and kelp holdfasts..."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "dropstone"@en .


:epimatrix a skos:Concept ;
    schema:citation "NADM SLTTs 2004, Dickinson,1970"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :matrix ;
    skos:definition "A type of matrix consisting of inhomogeneous interstitial materials grown in originally open interstices during diagenesis,but lacking the homogeneity and clear textural evidence of pore-filling needed to classify as phyllosilicate cement."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "epimatrix"@en .


:floating_clast a skos:Concept ;
    schema:citation "Pettijohn et al.,1972,p. 90, SMR"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :structural_part ;
    skos:definition "Constituent is a clast that has no visible contacts with other clasts. Interpreted to be largely or completly immersed in matrix or cement."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "floating clast"@en .


:granular_framework_constituent a skos:Concept ;
    schema:citation "Jackson,1997,p. 250"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :particulate_constituent ;
    skos:definition "Constituent forms a rigid arrangement of particles that support one another at their points of contact...constituting a mechanically firm structure capable of supporting open pore spaces,although interstices may be occupied by cement or matrix"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "granular framework constituent"@en .


:groundmass_constituent a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :interstitial_constituent ;
    skos:definition "Constituent forms an interconnected network of material enclosing other constituents that are disguished by larger grain size."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "groundmass constituent"@en .


