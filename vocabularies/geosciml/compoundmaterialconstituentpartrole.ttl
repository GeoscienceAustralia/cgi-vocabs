    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:prefLabel "crystalline framework constituent"@en .


<http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/dropstone> a skos:Concept ;
    dcterms:provenance "NADM SLTTs 2004, Jackson,1997,p 193"@en ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/structural_part> ;
    skos:definition "An oversized stone in laminated sediment that depresses the underlying laminae and may be covered by -draped laminae. Most dropstones originate through ice-rafting, other sources are floating tree roots and kelp holdfasts..."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:prefLabel "dropstone"@en .


<http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/epimatrix> a skos:Concept ;
    dcterms:provenance "NADM SLTTs 2004, Dickinson,1970"@en ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/matrix> ;
    skos:definition "A type of matrix consisting of inhomogeneous interstitial materials grown in originally open interstices during diagenesis,but lacking the homogeneity and clear textural evidence of pore-filling needed to classify as phyllosilicate cement."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:prefLabel "epimatrix"@en .


<http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/floating_clast> a skos:Concept ;
    dcterms:provenance "Pettijohn et al.,1972,p. 90, SMR"@en ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/structural_part> ;
    skos:definition "Constituent is a clast that has no visible contacts with other clasts. Interpreted to be largely or completly immersed in matrix or cement."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:prefLabel "floating clast"@en .


<http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/granular_framework_constituent> a skos:Concept ;
    dcterms:provenance "Jackson,1997,p. 250"@en ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/particulate_constituent> ;
    skos:definition "Constituent forms a rigid arrangement of particles that support one another at their points of contact...constituting a mechanically firm structure capable of supporting open pore spaces,although interstices may be occupied by cement or matrix"@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:prefLabel "granular framework constituent"@en .


<http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/groundmass_constituent> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/compoundmaterialconstituentpartrole/interstitial_constituent> ;
    skos:definition "Constituent forms an interconnected network of material enclosing other constituents that are disguished by larger grain size."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compoundmaterialconstituentpartrole> ;
    skos:prefLabel "groundmass constituent"@en .


