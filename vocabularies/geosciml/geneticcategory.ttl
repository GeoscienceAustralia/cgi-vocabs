PREFIX : <http://resource.geosciml.org/classifier/cgi/geneticcategory/>
PREFIX cs: <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory>
PREFIX dt: <http://resource.geosciml.org/datatype/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

cs:
    a skos:ConceptScheme ;
    schema:keywords "GeoSciML" ;
    schema:dateCreated "2009-07-14"^^xsd:date ;
    schema:creator <https://linked.data.gov.au/org/cgi-gtwg> ;
    schema:dateModified "2025-06-04"^^xsd:date ;
    schema:citation "Original set of terms from the GeosciML standard" ;
    schema:publisher <https://linked.data.gov.au/org/cgi> ;
    schema:citation "http://www.opengis.net/doc/geosciml/4.1"^^xsd:anyURI ;
    skos:changeNote
        "2009-07-14. Generate SKOS encoding of vocabulary from Excel spreadsheet."@en,
        "2009-12-07 SMR Update metadata properties for version, creator, title, and format. Change skos:HistoryNote to dc:source for information on origin of terms and definitions."@en,
        "2010-02-08 SMR add prefLabel strings; they were missing up to this time. Add and modify hierarchical relations to better represent concept hierachy. Add TopConcept assertions"@en,
        "2010-02-08. SMR Clean up namespace declarations so concept URI's follow CGI URN scheme; concepts have URN, are not document fragments."@en,
        "2010-11-28 SMR replace URN with http URI according to CGI URI scheme (see https://www.seegrid.csiro.au/wiki/bin/view/CGIModel/PersistentIdentifiersInGeoSciMLServices)"@en,
        "2012-02-07 SMR update URI to replace numeric final token with English-language string as in original URN scheme."@en,
        "2013-02-04 SMR Check update to 201211 version; add collection entity, check all pref labels are lower case, remove owl:NamedIndividual and Owl:Thing rdf:types. Add missing for missing definitions."@en,
        "2016-11-21 OLR regenerate SKOS-RDF file as part of migration to new vocabulary server. No changes to terms from the previous 2012 version."@en,
        "2020-06-23 NJC Added properties to ensure vocab matched Geoscience Australia's vocab profile (http://linked.data.gov.au/def/ga-skos-profile). Just annotation properties, no new content. Agents (creator/publisher) now not text but RDF resource. Dates (create/modified) derived from editorial notes & existing date properties."@en ,
        "2025-06-04 NJC Updates metadata to meet the VocPub 5.1 profile"@en ;
    skos:definition "This scheme specifies categories that summarize the geologic history of an Earth material. (ie, a genetic process classifier term), as defined by the IUGS Commission for Geoscience Information (CGI) Geoscience Terminology Working Group. Examples include igneous, sedimentary, metamorphic, shock metamorphic, volcanic, pyroclastic. By extension, this vocabulary includes all concepts in this conceptScheme, as well as concepts in any previous versions of the scheme."@en ;
    skos:editorialNote "This file contains the 2016 SKOS-RDF version of the CGI Genetic Category vocabulary.  Compilation and review in MS Excel spreadsheet, converted to MS Excel for SKOS generation using SKOS_for_GA_from_XLS_2016.03.xslt"@en ;
    skos:hasTopConcept :anthropogenic_genesis,
        :composite_process_genesis,
        :igneous_genesis,
        :igneous_sedimentary_genesis,
        :sedimentary_genesis ;
    skos:prefLabel "Genetic Category"@en
.


<http://resource.geosciml.org/classifier/cgi/geneticcategory>
    a skos:Collection ;
    schema:citation "this vocabulary" ;
    skos:definition "All Concepts in this vocabulary" ;
    skos:member
        :anthropogenic_genesis,
        :biological_sedimentary_genesis,
        :cataclastic_genesis,
        :chemical_sedimentary_genesis,
        :clastic_sedimentary_genesis,
        :composite_process_genesis,
        :contact_metamorphic_genesis,
        :deformation_genesis,
        :diagenetic_genesis,
        :dislocation_metamorphic_genesis,
        :ductile_deformation_genesis,
        :hypabyssal_intrusive_genesis,
        :igneous_extrusive_genesis,
        :igneous_genesis,
        :igneous_intrusive_genesis,
        :igneous_sedimentary_genesis,
        :impact_genesis,
        :local_metamorphic_genesis,
        :metaigneous_genesis,
        :metamorphic_genesis,
        :metamorphic_metasomatic_or_hydrothermal_genesis,
        :metaplutonic_genesis,
        :metasedimentary_genesis,
        :metasomatic_or_hydrothermal_genesis,
        :metavolcanic_genesis,
        :plutonic_genesis,
        :regional_metamorphic_genesis,
        :sedimentary_genesis,
        :subaerial_extrusive_genesis,
        :subaqueous_extrusive_genesis,
        :volcaniclastic_genesis,
        :weathering_genesis ;
    skos:prefLabel "Genetic Category - All Concepts"@en
.


:anthropogenic_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "Formation predominantly by human activity."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "anthropogenic genesis"@en ;
    skos:topConceptOf cs:
.


:composite_process_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "Formation in which more than one genetic process is dominant. This usually involves some combination of igneous, weathering, sedimentary, metamorphic, deformation or impact-related processes that better represents the origin of the material"@en ;
    skos:inScheme cs: ;
    skos:narrower
        :deformation_genesis,
        :impact_genesis,
        :metamorphic_metasomatic_or_hydrothermal_genesis,
        :weathering_genesis ;
    skos:prefLabel "composite process genesis"@en ;
    skos:topConceptOf cs:
.


:igneous_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "Formation predominantly by crystallisation from magma"@en ;
    skos:inScheme cs: ;
    skos:narrower
        :igneous_extrusive_genesis,
        :igneous_intrusive_genesis ;
    skos:prefLabel "igneous genesis"@en ;
    skos:topConceptOf cs:
.


:igneous_sedimentary_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "Formation by a combination of igneous and sedimentary processes"@en ;
    skos:inScheme cs: ;
    skos:narrower :volcaniclastic_genesis ;
    skos:prefLabel "igneous sedimentary genesis"@en ;
    skos:topConceptOf cs:
.


:sedimentary_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:definition "Formation predominantly by processes of erosion, mass wasting, transportation, deposition, precipitation and biogenic production that take place in Earth's hydrosphere and atmosphere."@en ;
    skos:inScheme cs: ;
    skos:narrower
        :biological_sedimentary_genesis,
        :chemical_sedimentary_genesis,
        :clastic_sedimentary_genesis,
        :diagenetic_genesis ;
    skos:prefLabel "sedimentary genesis"@en ;
    skos:topConceptOf cs:
.


:biological_sedimentary_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :sedimentary_genesis ;
    skos:definition "Formation predominantly by deposition of material produced by living organisms either as part of their body (e.g., exoskeleton, bone, pollen, wood) or through their activities (e.g., faecal pellets)."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "biological sedimentary genesis"@en
.


:cataclastic_genesis
    a skos:Concept ;
    schema:citation "based on Neuendorf et al 2005"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :deformation_genesis ;
    skos:definition "Formation predominantly by brittle deformation, i.e. the formation and growth of fractures and frictional sliding along fracture surfaces."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "cataclastic genesis"@en
.


:chemical_sedimentary_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :sedimentary_genesis ;
    skos:definition "Formation predominantly by direct chemical precipitation (e.g., evaporites, exhalative deposits)."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "chemical sedimentary genesis"@en
.


:clastic_sedimentary_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :sedimentary_genesis ;
    skos:definition "Formation predominantly by accumulation of particles (clasts) derived by weathering, erosion, or fragmentation of pre-existing rock or produced by chemical or biologically-mediated precipitation."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "clastic sedimentary genesis"@en
.


:contact_metamorphic_genesis
    a skos:Concept ;
    schema:citation "based on Smulikowski et al, 2003"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :metamorphic_genesis ;
    skos:definition "Formation predominantly by metamorphism due to the effect of a magma body on the rocks it intrudes."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "contact metamorphic genesis"@en
.


:deformation_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :composite_process_genesis ;
    skos:definition "Formation predominantly by strain-related processes resulting in changes of shape of a rock body, including folding, faulting, shearing, or fabric development, deformation may be either brittle or ductile. Local deformation metamorphic genesis related to a particular fault or shear zone is categorized as 'Dislocation metamorphic genesis'"@en ;
    skos:inScheme cs: ;
    skos:narrower
        :cataclastic_genesis,
        :dislocation_metamorphic_genesis,
        :ductile_deformation_genesis ;
    skos:prefLabel "deformation genesis"@en
.


:diagenetic_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :sedimentary_genesis ;
    skos:definition "Formation predominantly by chemical and physical changes subsequent to deposition of sediment, during and after lithification that occur under temperature and pressure conditions too low to be considered metamorphic."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "diagenetic genesis"@en
.


:dislocation_metamorphic_genesis
    a skos:Concept ;
    schema:citation "based on Smulikowski et al, 2003"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :deformation_genesis,
        :local_metamorphic_genesis ;
    skos:definition "Formation predominantly by metamorphism of local extent associated with fault zones or shear zones. 'Dislocation metamorphic genesis' is 'Deformation genesis' of local extent, explicitly associated with a particular fault or shear zone."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "dislocation metamorphic genesis"@en
.


:ductile_deformation_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :deformation_genesis ;
    skos:definition "Formation predominantly by deformation without loss of material continuity at the scale of observation."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "ductile deformation genesis"@en
.


:hypabyssal_intrusive_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :igneous_intrusive_genesis ;
    skos:definition "Formed by crystallisation close to the Earth's surface, characterized by more rapid cooling than plutonic setting to produce generally fine-grained intrusive igneous rock, commonly associated with co-magmatic volcanic rocks."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "hypabyssal intrusive genesis"@en
.


:igneous_extrusive_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :igneous_genesis ;
    skos:definition "Formation predominantly by crystallisation of magma at or immediately adjacent to Earth's surface."@en ;
    skos:inScheme cs: ;
    skos:narrower
        :subaerial_extrusive_genesis,
        :subaqueous_extrusive_genesis ;
    skos:prefLabel "igneous extrusive genesis"@en
.


:igneous_intrusive_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :igneous_genesis ;
    skos:definition "Formation predominantly by crystallisation of magma within the Earth."@en ;
    skos:inScheme cs: ;
    skos:narrower
        :hypabyssal_intrusive_genesis,
        :plutonic_genesis ;
    skos:prefLabel "igneous intrusive genesis"@en
.


:impact_genesis
    a skos:Concept ;
    schema:citation "Smulikowski et al, 2003"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :composite_process_genesis ;
    skos:definition "Formation predominantly by metamorphism related to the passage of a shock wave through a body of material, typically the result of impact of a planetary body (impactor) on a planetary surface (target)"@en ;
    skos:inScheme cs: ;
    skos:prefLabel "impact genesis"@en
.


:local_metamorphic_genesis
    a skos:Concept ;
    schema:citation "Smulikowski et al, 2003"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :metamorphic_genesis ;
    skos:definition "Formation predominantly by metamorphism that may be attributed to a localized cause, such as magmatic intrusion, faulting, metorite impact, combustion of naturally occuring substances (coal), or lightning."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "local metamorphic genesis"@en
.


:metaigneous_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :metamorphic_genesis ;
    skos:definition "Formation by metamorphism of an igneous protolith"@en ;
    skos:inScheme cs: ;
    skos:narrower
        :metaplutonic_genesis,
        :metavolcanic_genesis ;
    skos:prefLabel "metaigneous genesis"@en
.


:metamorphic_genesis
    a skos:Concept ;
    schema:citation "based on Neuendorf et al. 2005"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :metamorphic_metasomatic_or_hydrothermal_genesis ;
    skos:definition "Formation predominantly by closed-system changes in mineralogy, texture, or fabric of a rock in response to chemical and physical conditions that have been imposed below the surface zones of weathering and cementation (diagenesis), and that differ from the conditions under which the rocks in question originated. As defined here the changes in rock volume may include removal of some chemical constituents from the system (especially water and other volatile constituents). Note narrower use that Neuendorf et al. 2005, this vocabulary distinguishes metasomatism from metamorphism."@en ;
    skos:inScheme cs: ;
    skos:narrower
        :contact_metamorphic_genesis,
        :local_metamorphic_genesis,
        :metaigneous_genesis,
        :metasedimentary_genesis,
        :regional_metamorphic_genesis ;
    skos:prefLabel "metamorphic genesis"@en
.


:metamorphic_metasomatic_or_hydrothermal_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :composite_process_genesis ;
    skos:definition "Formation predominantly by changes in chemical, mineralogical, or structural properties of rocks in response to chemical and physical conditions that have been imposed below the surface zones of weathering and cementation (diagenesis), and that differ from the conditions under which the rocks in question originated."@en ;
    skos:inScheme cs: ;
    skos:narrower
        :metamorphic_genesis,
        :metasomatic_or_hydrothermal_genesis ;
    skos:prefLabel "metamorphic metasomatic or hydrothermal genesis"@en
.


:metaplutonic_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :metaigneous_genesis ;
    skos:definition "Formation by metamorphism of a plutonic igneous protolith."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "metaplutonic genesis"@en
.


:metasedimentary_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :metamorphic_genesis ;
    skos:definition "Formation by metamorphism of a sedimentary protolith."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "metasedimentary genesis"@en
.


:metasomatic_or_hydrothermal_genesis
    a skos:Concept ;
    schema:citation "based on Neuendorf et al 2005"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :metamorphic_metasomatic_or_hydrothermal_genesis ;
    skos:definition "Formation predominantly by open-system changes in chemical composition by reaction with an external source, typically involving chemical transport by a fluid medium flowing through the rock. Metasomatism typically involves introduction of chemical constituents into a rock volume."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "metasomatic or hydrothermal genesis"@en
.


:metavolcanic_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :metaigneous_genesis ;
    skos:definition "Formation by metamorphism of an extrusive igneous protolith."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "metavolcanic genesis"@en
.


:plutonic_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :igneous_intrusive_genesis ;
    skos:definition "Formation predominantly by crystallisation of magma far enough below Earth surface that complete crystallization of magma bodies forms holocrystalline medium to coarse grained igneous rock, wall rocks generally do not include volcanic products related to the magma, and some contact metamorphism is developed at intrusive contacts."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "plutonic genesis"@en
.


:regional_metamorphic_genesis
    a skos:Concept ;
    schema:citation "Smulikowski et al, 2003"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :metamorphic_genesis ;
    skos:definition "Formation predominantly by metamorphic processes affecting a large rock volume, associated with large-scale tectonic processes."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "regional metamorphic genesis"@en
.


:subaerial_extrusive_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :igneous_extrusive_genesis ;
    skos:definition "Formation predominantly by crystallisation of magma either in the open air or immediately adjacent to the land surface."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "subaerial extrusive genesis"@en
.


:subaqueous_extrusive_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :igneous_extrusive_genesis ;
    skos:definition "Formation predominantly by crystallisation of magma under water or ice."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "subaqueous extrusive genesis"@en
.


:volcaniclastic_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :igneous_sedimentary_genesis ;
    skos:definition "Formation by a combination of extrusive igneous activity and sedimentary transport and deposition."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "volcaniclastic genesis"@en
.


:weathering_genesis
    a skos:Concept ;
    schema:citation "This vocabulary"@en ;
    rdfs:isDefinedBy cs: ;
    skos:broader :composite_process_genesis ;
    skos:definition "Formation predominantly by the physical, chemical or biological alteration of rock or sediment at or near the Earth surface, connotes involvement of processes related to the atmosphere (weather...)."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "weathering genesis"@en
.

