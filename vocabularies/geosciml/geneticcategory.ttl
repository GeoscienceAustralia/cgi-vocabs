<http://resource.geosciml.org/classifier/cgi/geneticcategory/anthropogenic_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:definition "Formation predominantly by human activity."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "anthropogenic genesis"@en ;
    skos:topConceptOf <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/biological_sedimentary_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/sedimentary_genesis> ;
    skos:definition "Formation predominantly by deposition of material produced by living organisms either as part of their body (e.g., exoskeleton, bone, pollen, wood) or through their activities (e.g., faecal pellets)."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "biological sedimentary genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/cataclastic_genesis> a skos:Concept ;
    dcterms:provenance "based on Neuendorf et al 2005"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/deformation_genesis> ;
    skos:definition "Formation predominantly by brittle deformation, i.e. the formation and growth of fractures and frictional sliding along fracture surfaces."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "cataclastic genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/chemical_sedimentary_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/sedimentary_genesis> ;
    skos:definition "Formation predominantly by direct chemical precipitation (e.g., evaporites, exhalative deposits)."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "chemical sedimentary genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/clastic_sedimentary_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/sedimentary_genesis> ;
    skos:definition "Formation predominantly by accumulation of particles (clasts) derived by weathering, erosion, or fragmentation of pre-existing rock or produced by chemical or biologically-mediated precipitation."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "clastic sedimentary genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/contact_metamorphic_genesis> a skos:Concept ;
    dcterms:provenance "based on Smulikowski et al, 2003"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_genesis> ;
    skos:definition "Formation predominantly by metamorphism due to the effect of a magma body on the rocks it intrudes."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "contact metamorphic genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/diagenetic_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/sedimentary_genesis> ;
    skos:definition "Formation predominantly by chemical and physical changes subsequent to deposition of sediment, during and after lithification that occur under temperature and pressure conditions too low to be considered metamorphic."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "diagenetic genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/dislocation_metamorphic_genesis> a skos:Concept ;
    dcterms:provenance "based on Smulikowski et al, 2003"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/deformation_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/local_metamorphic_genesis> ;
    skos:definition "Formation predominantly by metamorphism of local extent associated with fault zones or shear zones. 'Dislocation metamorphic genesis' is 'Deformation genesis' of local extent, explicitly associated with a particular fault or shear zone."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "dislocation metamorphic genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/ductile_deformation_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/deformation_genesis> ;
    skos:definition "Formation predominantly by deformation without loss of material continuity at the scale of observation."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "ductile deformation genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/hypabyssal_intrusive_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_intrusive_genesis> ;
    skos:definition "Formed by crystallisation close to the Earth's surface, characterized by more rapid cooling than plutonic setting to produce generally fine-grained intrusive igneous rock, commonly associated with co-magmatic volcanic rocks."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "hypabyssal intrusive genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/impact_genesis> a skos:Concept ;
    dcterms:provenance "Smulikowski et al, 2003"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/composite_process_genesis> ;
    skos:definition "Formation predominantly by metamorphism related to the passage of a shock wave through a body of material, typically the result of impact of a planetary body (impactor) on a planetary surface (target)"@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "impact genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/metaplutonic_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/metaigneous_genesis> ;
    skos:definition "Formation by metamorphism of a plutonic igneous protolith."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "metaplutonic genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/metasedimentary_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_genesis> ;
    skos:definition "Formation by metamorphism of a sedimentary protolith."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "metasedimentary genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/metasomatic_or_hydrothermal_genesis> a skos:Concept ;
    dcterms:provenance "based on Neuendorf et al 2005"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_metasomatic_or_hydrothermal_genesis> ;
    skos:definition "Formation predominantly by open-system changes in chemical composition by reaction with an external source, typically involving chemical transport by a fluid medium flowing through the rock. Metasomatism typically involves introduction of chemical constituents into a rock volume."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "metasomatic or hydrothermal genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/metavolcanic_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/metaigneous_genesis> ;
    skos:definition "Formation by metamorphism of an extrusive igneous protolith."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "metavolcanic genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/plutonic_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_intrusive_genesis> ;
    skos:definition "Formation predominantly by crystallisation of magma far enough below Earth surface that complete crystallization of magma bodies forms holocrystalline medium to coarse grained igneous rock, wall rocks generally do not include volcanic products related to the magma, and some contact metamorphism is developed at intrusive contacts."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "plutonic genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/regional_metamorphic_genesis> a skos:Concept ;
    dcterms:provenance "Smulikowski et al, 2003"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_genesis> ;
    skos:definition "Formation predominantly by metamorphic processes affecting a large rock volume, associated with large-scale tectonic processes."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "regional metamorphic genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/subaerial_extrusive_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_extrusive_genesis> ;
    skos:definition "Formation predominantly by crystallisation of magma either in the open air or immediately adjacent to the land surface."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "subaerial extrusive genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/subaqueous_extrusive_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_extrusive_genesis> ;
    skos:definition "Formation predominantly by crystallisation of magma under water or ice."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "subaqueous extrusive genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/volcaniclastic_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_sedimentary_genesis> ;
    skos:definition "Formation by a combination of extrusive igneous activity and sedimentary transport and deposition."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "volcaniclastic genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/weathering_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/composite_process_genesis> ;
    skos:definition "Formation predominantly by the physical, chemical or biological alteration of rock or sediment at or near the Earth surface, connotes involvement of processes related to the atmosphere (weather...)."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "weathering genesis"@en .

