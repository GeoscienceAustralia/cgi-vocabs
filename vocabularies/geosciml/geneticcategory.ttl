<http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_sedimentary_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:definition "Formation by a combination of igneous and sedimentary processes"@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:narrower <http://resource.geosciml.org/classifier/cgi/geneticcategory/volcaniclastic_genesis> ;
    skos:prefLabel "igneous sedimentary genesis"@en ;
    skos:topConceptOf <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/local_metamorphic_genesis> a skos:Concept ;
    dcterms:provenance "Smulikowski et al, 2003"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_genesis> ;
    skos:definition "Formation predominantly by metamorphism that may be attributed to a localized cause, such as magmatic intrusion, faulting, metorite impact, combustion of naturally occuring substances (coal), or lightning."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:prefLabel "local metamorphic genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_extrusive_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_genesis> ;
    skos:definition "Formation predominantly by crystallisation of magma at or immediately adjacent to Earth's surface."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:narrower <http://resource.geosciml.org/classifier/cgi/geneticcategory/subaerial_extrusive_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/subaqueous_extrusive_genesis> ;
    skos:prefLabel "igneous extrusive genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:definition "Formation predominantly by crystallisation from magma"@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:narrower <http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_extrusive_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_intrusive_genesis> ;
    skos:prefLabel "igneous genesis"@en ;
    skos:topConceptOf <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_intrusive_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/igneous_genesis> ;
    skos:definition "Formation predominantly by crystallisation of magma within the Earth."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:narrower <http://resource.geosciml.org/classifier/cgi/geneticcategory/hypabyssal_intrusive_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/plutonic_genesis> ;
    skos:prefLabel "igneous intrusive genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/metaigneous_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_genesis> ;
    skos:definition "Formation by metamorphism of an igneous protolith"@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:narrower <http://resource.geosciml.org/classifier/cgi/geneticcategory/metaplutonic_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/metavolcanic_genesis> ;
    skos:prefLabel "metaigneous genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_metasomatic_or_hydrothermal_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/composite_process_genesis> ;
    skos:definition "Formation predominantly by changes in chemical, mineralogical, or structural properties of rocks in response to chemical and physical conditions that have been imposed below the surface zones of weathering and cementation (diagenesis), and that differ from the conditions under which the rocks in question originated."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:narrower <http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/metasomatic_or_hydrothermal_genesis> ;
    skos:prefLabel "metamorphic metasomatic or hydrothermal genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/deformation_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/composite_process_genesis> ;
    skos:definition "Formation predominantly by strain-related processes resulting in changes of shape of a rock body, including folding, faulting, shearing, or fabric development, deformation may be either brittle or ductile. Local deformation metamorphic genesis related to a particular fault or shear zone is categorized as 'Dislocation metamorphic genesis'"@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:narrower <http://resource.geosciml.org/classifier/cgi/geneticcategory/cataclastic_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/dislocation_metamorphic_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/ductile_deformation_genesis> ;
    skos:prefLabel "deformation genesis"@en .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/composite_process_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:definition "Formation in which more than one genetic process is dominant. This usually involves some combination of igneous, weathering, sedimentary, metamorphic, deformation or impact-related processes that better represents the origin of the material"@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:narrower <http://resource.geosciml.org/classifier/cgi/geneticcategory/deformation_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/impact_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_metasomatic_or_hydrothermal_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/weathering_genesis> ;
    skos:prefLabel "composite process genesis"@en ;
    skos:topConceptOf <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/sedimentary_genesis> a skos:Concept ;
    dcterms:provenance "This vocabulary"@en ;
    skos:definition "Formation predominantly by processes of erosion, mass wasting, transportation, deposition, precipitation and biogenic production that take place in Earth's hydrosphere and atmosphere."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:narrower <http://resource.geosciml.org/classifier/cgi/geneticcategory/biological_sedimentary_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/chemical_sedimentary_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/clastic_sedimentary_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/diagenetic_genesis> ;
    skos:prefLabel "sedimentary genesis"@en ;
    skos:topConceptOf <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> .

<http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_genesis> a skos:Concept ;
    dcterms:provenance "based on Neuendorf et al. 2005"@en ;
    skos:broader <http://resource.geosciml.org/classifier/cgi/geneticcategory/metamorphic_metasomatic_or_hydrothermal_genesis> ;
    skos:definition "Formation predominantly by closed-system changes in mineralogy, texture, or fabric of a rock in response to chemical and physical conditions that have been imposed below the surface zones of weathering and cementation (diagenesis), and that differ from the conditions under which the rocks in question originated. As defined here the changes in rock volume may include removal of some chemical constituents from the system (especially water and other volatile constituents). Note narrower use that Neuendorf et al. 2005, this vocabulary distinguishes metasomatism from metamorphism."@en ;
    skos:inScheme <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> ;
    skos:narrower <http://resource.geosciml.org/classifier/cgi/geneticcategory/contact_metamorphic_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/local_metamorphic_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/metaigneous_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/metasedimentary_genesis>,
        <http://resource.geosciml.org/classifier/cgi/geneticcategory/regional_metamorphic_genesis> ;
    skos:prefLabel "metamorphic genesis"@en .

<http://resource.geosciml.org/classifierscheme/cgi/2016.01/geneticcategory> a skos:ConceptScheme ;
