<http://resource.geosciml.org/classifier/cgi/compositioncategory/sub_feldspathic_detrital_mode> a skos:Concept ;
    dcterms:provenance "Folk1968"@en ;
    rdfs:isDefinedBy <http://resource.geosciml.org/classifierscheme/cgi/2016.01/compositioncategory> ;
