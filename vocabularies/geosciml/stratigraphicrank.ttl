    a skos:Collection ;
    rdfs:isDefinedBy cs: ;
    skos:definition "All Concepts in this vocabulary" ;
    skos:historyNote "this vocabulary" ;
    skos:inScheme cs: ;
    skos:member
        <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/bed> ,
        <http://resource.geosciml.org/classifier/cgi/stratigraphicrank/complex> ,
